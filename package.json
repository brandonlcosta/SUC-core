{
  "name": "suc-core-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run sync:outputs && concurrently \"npm run watch:pipeline\" \"vite\"",
    "sync:outputs": "node ../scripts/syncOutputs.js",
    "watch:pipeline": "node ../scripts/watchPipeline.js",
    "build": "vite build",
    "preview": "vite preview",

    "ws:ticks": "node ../backend/server/demoServer.js",
    "demo:map": "node ../tests/demoMapEvents.js",

    "dev:all": "concurrently \"npm run ws:ticks\" \"npm run dev\"",
    "dev:all:demo": "concurrently \"npm run ws:ticks\" \"npm run demo:map\" \"npm run dev\""
  },
  "dependencies": {
    "@turf/helpers": "^7.2.0",
    "mapbox-gl": "^3.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.0",
    "fs-extra": "^11.2.0",
    "chokidar": "^3.6.0"
  }
}
