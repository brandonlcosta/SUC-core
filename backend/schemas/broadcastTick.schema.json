{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Broadcast Tick",
  "type": "object",
  "required": ["ts", "ticker", "hud", "highlights", "recap"],
  "properties": {
    "ts": { "type": "number" },
    "ticker": { "type": "string" },
    "hud": { "type": "object" },
    "highlights": {
      "type": "array",
      "items": { "type": "object" }
    },
    "recap": { "type": "object" }
  },
  "additionalProperties": false
}
